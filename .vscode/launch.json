  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Game",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/GAME_APPLICATION.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable break on exception",
          "text": "-catch throw",
          "ignoreFailures": true
        }
      ],
      "logging": {
        "engineLogging": true
      }
    }
  ]
}
